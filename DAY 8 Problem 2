link: https://leetcode.com/problems/excel-sheet-column-number/submissions/1627808427/
class Solution {
public:
    int titleToNumber(string columnTitle) {
        int result=0;
        for(char c: columnTitle){
            int d= c -'A' + 1;
            result=result*26 +d;

        }
        return result;
    }
};
